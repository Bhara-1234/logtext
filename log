# Logging Configuration

# File appender configuration
logging:
  file:
    path: C:/temp/pro3.log
  level:
    root: INFO

# Console appender configuration
console:
  enabled: true
  level: INFO  # Set the log level for console output

loggers:
  root:
    level: INFO  # Set the root logger's log level to INFO



